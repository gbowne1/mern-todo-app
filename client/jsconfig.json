{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@services/*": ["services/*"],
      "@assets/*": ["assets/*"],
      "@theme/*": ["theme/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
